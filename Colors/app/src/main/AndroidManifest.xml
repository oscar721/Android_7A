<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.colors">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">

        <activity android:name=".HelpActivity" android:label="@string/lblHelp" android:parentActivityName=".Palette">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value=".Palette" />

            <intent-filter>
                <action android:name="android.intent.action.HelpActivity" />

                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>

        <activity android:name=".AboutofActivity" android:label="@string/lblOpAboutof" android:parentActivityName=".Palette">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value=".Palette" />

            <intent-filter>
                <action android:name="android.intent.action.AboutofActivity" />

                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>

        <activity android:name=".Palette">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity android:name=".MainActivity" />

    </application>

</manifest>